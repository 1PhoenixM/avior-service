<%
/**
 * Boilerplate attribute
 *
 * This is the template used when creating attributes
 * for models generated through the CLI
 * e.g. `sails generate model foo attrName`
 *
 */
%>
<%if (lang === 'js') {%>    <%= name %> : { type: '<%= type %>' }
<%} else if (lang === 'coffee'){ %>    <%=name%>:
      type: '<%= type %>'
<%}%>
